{
  "name": "lkt-anchor",
  "version": "1.0.1",
  "description": "",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/lkt-anchor.es.js",
