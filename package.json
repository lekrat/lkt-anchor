    "rollup": "^4.9.6",
    "typescript": "^5.3.3",
    "vite": "^3.2.8",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "lkt-modal-confirm": "^1.0.0",
    "lkt-ts-interfaces": "^1.0.2",
    "vue": "^3.4",
    "vue-router": "^4.2.5"
